    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "s3",
    "aws",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "adm-zip": "^0.5.16",
    "commander": "^11.0.0",
    "dotenv": "^16.4.7",
    "pdfreader": "^3.0.7",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^20.0.0",
    "@types/xml2js": "^0.4.14",
    "typescript": "^5.0.0"
  }
